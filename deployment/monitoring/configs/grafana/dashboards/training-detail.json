{
  "uid": "training-detail",
  "title": "Training Detail",
  "tags": [
    "mlops",
    "training",
    "detail",
    "drilldown"
  ],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "editable": true,
  "annotations": {
    "list": []
  },
  "templating": {
    "list": [
      {
        "name": "experiment_id",
        "label": "Experiment ID",
        "type": "textbox",
        "current": {
          "text": ".*",
          "value": ".*"
        },
        "query": ".*",
        "hide": 0
      },
      {
        "name": "run_id",
        "label": "Run ID",
        "type": "textbox",
        "current": {
          "text": ".*",
          "value": ".*"
        },
        "query": ".*",
        "hide": 0
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "üìâ Training Loss Curve",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "training_loss{run_id=~\"$run_id\", experiment_id=~\"$experiment_id\"}",
          "legendFormat": "loss ({{run_id}})",
          "refId": "A"
        },
        {
          "expr": "training_eval_loss{run_id=~\"$run_id\", experiment_id=~\"$experiment_id\"}",
          "legendFormat": "eval_loss ({{run_id}})",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "min": 0,
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      }
    },
    {
      "id": 2,
      "title": "üìà Learning Rate Schedule",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "training_learning_rate{run_id=~\"$run_id\", experiment_id=~\"$experiment_id\"}",
          "legendFormat": "lr ({{run_id}})",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "min": 0,
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      }
    },
    {
      "id": 3,
      "title": "üñ•Ô∏è GPU ÏÇ¨Ïö©Î•†",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "training_gpu_utilization{run_id=~\"$run_id\", experiment_id=~\"$experiment_id\"}",
          "legendFormat": "GPU {{gpu_id}} ÏÇ¨Ïö©Î•†",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "fillOpacity": 20
          },
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "yellow",
                "value": 50
              },
              {
                "color": "green",
                "value": 80
              }
            ]
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      }
    },
    {
      "id": 4,
      "title": "üíæ GPU Î©îÎ™®Î¶¨ ÏÇ¨Ïö©Îüâ",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "targets": [
        {
          "expr": "training_gpu_memory_used_bytes{run_id=~\"$run_id\", experiment_id=~\"$experiment_id\"}",
          "legendFormat": "GPU {{gpu_id}} ÏÇ¨Ïö© Î©îÎ™®Î¶¨",
          "refId": "A"
        },
        {
          "expr": "training_gpu_memory_total_bytes{run_id=~\"$run_id\", experiment_id=~\"$experiment_id\"}",
          "legendFormat": "GPU {{gpu_id}} Ï†ÑÏ≤¥ Î©îÎ™®Î¶¨",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "min": 0,
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "fillOpacity": 15
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      }
    },
    {
      "id": 5,
      "title": "üî¢ Training Step",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 16
      },
      "targets": [
        {
          "expr": "training_step{run_id=~\"$run_id\", experiment_id=~\"$experiment_id\"}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "thresholds": {
            "steps": [
              {
                "color": "blue",
                "value": null
              }
            ]
          }
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area"
      }
    },
    {
      "id": 6,
      "title": "‚è±Ô∏è Epoch",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 16
      },
      "targets": [
        {
          "expr": "training_epoch{run_id=~\"$run_id\", experiment_id=~\"$experiment_id\"}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "thresholds": {
            "steps": [
              {
                "color": "purple",
                "value": null
              }
            ]
          }
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area"
      }
    },
    {
      "id": 7,
      "title": "‚öôÔ∏è ÌïòÏù¥ÌçºÌååÎùºÎØ∏ÌÑ∞ Ï†ïÎ≥¥",
      "type": "table",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 16
      },
      "datasource": {
        "type": "loki",
        "uid": "loki"
      },
      "targets": [
        {
          "expr": "{job=\"training\"} | json | run_id=~\"$run_id\" | event=\"hyperparameters\"",
          "refId": "A"
        }
      ],
      "transformations": [
        {
          "id": "extractFields",
          "options": {
            "source": "Line",
            "format": "json"
          }
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "id": true,
              "tsNs": true,
              "labels": true
            }
          }
        }
      ],
      "options": {
        "showHeader": true,
        "sortBy": []
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "left"
          }
        }
      }
    }
  ]
}
